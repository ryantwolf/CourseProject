{
    "manifest_version": 3,
    "name": "CS 410 Chrome Extension",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "action": {
        "default_popup": "Popup.html",
        "default_title" : "410 Projects"
    },
    "permissions": [
        "bookmarks"
    ],
    "host_permissions": [
        "https://*/"
    ]
}